#functions
def person(name, age):
    print(name)
    print(age)

#calling a function type1
person("manasa", 27)

#calling a function type2
person(28,"Manasa")

#calling a function type 3
person(name="manasa", age="27")

#calling a function type 4
# person("navin")

#assigning a default value
def book_store(name, cost = 25):
    print(name)
    print(cost)

#calling a function with default value
book_store('analytics')

#passing values
def sum(a,b):
    c = a+b
    print(c)

sum(10,15)

# #passing multiple values
# def sum(a,*b):
#     c = a+b
#     print(c)
#
# sum(10,20,30)

def sum(*b):
    c = 0
    for i in b:
        c = c+i
    print(c)

sum(10,15,5)
